  author = {Verschaffelt, Pieter and Van Den Bossche, Tim and Gabriel, Wassim and Burdukiewicz, Michał and Soggiu, Alessio and Martens, Lennart and Renard, Bernhard Y. and Schiebenhoefer, Henning and Mesuere, Bart},
