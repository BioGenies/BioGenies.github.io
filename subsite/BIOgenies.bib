  author = {{Badaczewska-Dawid}, Aleksandra E. and Kuriata, Aleksander and {Pintado-Grima}, Carlos and {Garcia-Pardo}, Javier and Burdukiewicz, Michał and Iglesias, Valentín and Kmiecik, Sebastian and Ventura, Salvador},
