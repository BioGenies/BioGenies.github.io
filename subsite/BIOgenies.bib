}

@article{burdukiewicz_algorithms_2018,
  title = {Algorithms for Automated Detection of Hook Effect-Bearing Amplification Curves},
  author = {Burdukiewicz, Michał and Spiess, Andrej-Nikolai and Blagodatskikh, Konstantin A. and Lehmann, Werner and Schierack, Peter and Rödiger, Stefan},
