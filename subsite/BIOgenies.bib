}

@article{zareba-koziol_intracellular_2022,
  title = {Intracellular {{Protein S-Nitrosylation}}—{{A Cells Response}} to {{Extracellular S100B}} and {{RAGE Receptor}}},
  author = {{Zaręba-Kozioł}, Monika and Burdukiewicz, Michał and {Wysłouch-Cieszyńska}, Aleksandra},
  year = {2022},
